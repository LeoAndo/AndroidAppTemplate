<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ShapeAppearanceOverlay.AndroidAppTemplate" parent="" />

    <style name="ShapeAppearanceOverlay.AndroidAppTemplate.CircleImageView">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyBottomRight">cut</item>
        <item name="cornerSizeTopLeft">20%</item>
        <item name="cornerSizeBottomRight">28%</item>
    </style>

    <style name="Divider" parent="" />

    <style name="Divider.Horizontal">
        <item name="android:layout_height">0.33dp</item>
        <item name="android:background">?attr/dividerHorizontal</item>
    </style>

    <style name="CircularProgressIndicator" parent="Widget.MaterialComponents.CircularProgressIndicator">
        <item name="android:indeterminate">true</item>
        <item name="indicatorColor">@array/progress_colors</item>
        <item name="indeterminateAnimationType">contiguous</item>
        <item name="indicatorSize">80dp</item>
    </style>

    <style name="TextInputLayoutDropDown" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="android:textColorHint">?attr/colorPrimary</item>
        <item name="hintTextColor">?attr/colorPrimary</item>
        <item name="boxStrokeColor">?attr/colorPrimary</item>
    </style>

    <style name="MyTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox" />

    <style name="AutoCompleteTextView" parent="Widget.AppCompat.AutoCompleteTextView" />

    <style name="AutoCompleteTextView.DropDown">
        <item name="android:editable">false</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>